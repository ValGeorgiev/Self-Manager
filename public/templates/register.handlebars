<div class="container">
    <div class="row">
        <div class="col-md-10">
            <h1>Register</h1>
        </div>
        <div class="col-md-1">
            <a href="#/register">Register</a>
        </div>
        <div class="col-md-1">
            <a href="#/login">Login </a>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-sm-12">

            <div class="form-group">
                <label for="tb-username">UserName: </label>
                <input id="tb-username" class="form-control" type="text"/>
            </div>
            <div class="form-group">
                <label for="tb-pass">Password: </label>
                <input id="tb-pass" class="form-control" type="password"/>
            </div>
            <div class="form-group">
                <label for="tb-pass-check">Repeat Password: </label>
                <input id="tb-pass-check" class="form-control" type="password"/>
            </div>

            <button class="btn btn-success" id="btn-register" disabled>Register</button>

        </div>
    </div>
</div>