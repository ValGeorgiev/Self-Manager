<div class="container">
    <div class="row">
        <div class="col-md-11">
            <h2>Add new Event</h2>
        </div>
        <div class="col-md-1">
            <a id="logOut" href="#/login">Log Out</a>
        </div>
    </div>
</div>


<div class="form-group">
    <label for="tb-event-title">Title: </label>
    <input id="tb-event-title" class="form-control" type="text"/>
</div>
<div class="form-group">
    <label for="tb-event-category">Category: </label>
    <input id="tb-event-category" class="form-control" type="text"/>
</div>
<div class="form-group">
    <label for="tb-event-date">Date: </label>
    <input id="tb-event-date" class="form-control"/>
</div>
<div class="form-group">
    <label for="tb-event-time">Time: </label>
    <input id="tb-event-time" class="form-control"/>
</div>
<div class="form-group">
    <label for="tb-event-description">Description: </label>
    <input id="tb-event-description" class="form-control"/>
</div>


<button class="btn btn-success" id="btn-event-add">Add</button>