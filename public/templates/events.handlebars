<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Events</h2>
        </div>
    </div>
    <div class="row">

        <nav class="navbar navbar-default">
            <div class="col-md-11">
                <ul class="nav navbar-nav">
                    <li><a href="#/todos">My TODOs</a></li>
                    <li><a href="#/events">My Events</a></li>
                </ul>
            </div>

            <div class="col-md-1">
                <a id="logOut" href="#/login">Log Out</a>
            </div>
        </nav>

    </div>
</div>
<a href="#/events/add">Add new Event</a>
{{#if this }}
    {{#each this}}
        <h3><a href="#/events?category={{category}}">{{category}}</a></h3>
        <ul>
            {{#each events}}
                <li class="event-item" data-id="{{id}}">
                    <label>
                        {{title}}
                    </label>
                </li>
            {{/each}}
        </ul>
    {{/each}}

{{else}}
    <h3>No Events</h3>
{{/if}}