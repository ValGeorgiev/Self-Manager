<h2>Events</h2>
<a href="#/events/add">Add new Event</a>
{{#if this }}
    {{#each this}}
        <h3><a href="#/events?category={{category}}">{{category}}</a></h3>
        <ul>
            {{#each events}}
                <li class="event-item" data-id="{{id}}">
                    <label>
                        {{title}}
                    </label>
                </li>
            {{/each}}
        </ul>
    {{/each}}

{{else}}
    <h3>No Events</h3>
{{/if}}